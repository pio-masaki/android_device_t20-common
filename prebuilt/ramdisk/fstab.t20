# Android fstab file.
#<src>                                        <mnt_point> <type>  <mnt_flags>             <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/platform/sdhci-tegra.3/by-name/AP  /system     ext4     ro                      wait
/dev/block/platform/sdhci-tegra.3/by-name/UA  /data       ext4     noatime,nosuid,nodev    wait
/dev/block/platform/sdhci-tegra.3/by-name/CC  /cache      ext4     noatime,nosuid,nodev    wait

